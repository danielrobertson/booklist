generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model book_list_xref {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  list_id    BigInt?
  book_id    BigInt?
  books      books?   @relation(fields: [book_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  lists      lists?   @relation(fields: [list_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model books {
  id             BigInt           @unique @default(autoincrement())
  created_at     DateTime         @default(now()) @db.Timestamptz(6)
  google_book_id String
  book_list_xref book_list_xref[]

  @@id([id, google_book_id])
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model lists {
  id             BigInt           @id @unique @default(autoincrement())
  created_at     DateTime         @default(now()) @db.Timestamptz(6)
  list_id        String?          @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name           String?
  description    String?
  book_list_xref book_list_xref[]
}
